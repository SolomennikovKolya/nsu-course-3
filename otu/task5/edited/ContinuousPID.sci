n = 4;
T0 = 0.54;
K = 0.5;
Ti = 1;
Td = Ti/4;
Ts = Td/8;

s=poly(0,'s');
W1=2*(Td*s/(1+Ts*s)+1+1/(Ti*s))*K*(1/(1+T0*s)^n);
W=W1/(1+W1);
a = [1, 4.22, 8.74, 11.61, 10.65, 6.81, 2.98, 0.86, 0.15, 0.0146028];
a = a / 0.0005565;
disp('W:', W);
disp('a:', a);
disp('WW:', W.den);

I = eye(9,9);
A = [[zeros(1,9); I], -a'];
disp('A:',A);

I=eye(A);
H=lyap(A,-I,'c');

l=spec(H);
disp('Собственные числа H:', l);
k = norm(H,2);
disp('Запас устойчивости:', k); 
